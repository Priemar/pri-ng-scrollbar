{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "pri-ng-scrollbar": [
        "dist/pri-ng-scrollbar"
      ],
      "pri-ng-scrollbar/*": [
        "dist/pri-ng-scrollbar/*"
      ]
    }
  }
}