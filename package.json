{
  "name": "pri-ng-scrollbar-demo",
  "version": "5.0.0",
  "license": "MIT",
  "description": "Custom overlay scrollbars using native scrolling mechanism, for angular outside angular change detection. now also usable as web-component in all web applications",
  "homepage": "https://github.com/Priemar/pri-ng-scrollbar",
  "author": {
    "name": "Mario Prieschl",
    "url": "http://github.com/Priemar"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Priemar/pri-ng-scrollbar.git"
  },
  "bugs": {
    "url": "https://github.com/Priemar/pri-ng-scrollbar/issues"
  },
  "keywords": [
    "scrollbar",
    "scroll",
    "custom",
    "native",
    "optimized",
    "webcomponent",
    "angular",
    "performance"
  ],
  "scripts": {
    "update": "ng update",
    "update:all": "ng update --all --force",
    "start": "ng run pri-ng-scrollbar-demo:serve --host 0.0.0.0 --port 4200",
    "start:lib": "ng run pri-ng-scrollbar:build:production --watch",
    "build": "npm run build:lib && ng run pri-ng-scrollbar-demo:build:production --base-href https://priemar.github.io/pri-ng-scrollbar/",
    "build:lib": "ng run pri-ng-scrollbar:build:production && node build/copy.theme.js && npm run build:web-component",
    "build:web-component": "ng run wc-pri-ng-scrollbar:build:production && node projects/web-component/bundle.js",
    "publish": "npm publish dist/pri-ng-scrollbar"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.3",
    "@angular/cdk": "^10.0.1",
    "@angular/common": "~10.0.3",
    "@angular/compiler": "~10.0.3",
    "@angular/core": "~10.0.3",
    "@angular/elements": "^10.0.3",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~10.0.3",
    "@angular/material": "^10.0.1",
    "@angular/platform-browser": "~10.0.3",
    "@angular/platform-browser-dynamic": "~10.0.3",
    "@angular/router": "^10.0.3",
    "@webcomponents/custom-elements": "^1.4.1",
    "core-js": "^3.6.5",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.1",
    "@angular-devkit/build-ng-packagr": "~0.1000.1",
    "@angular/cli": "~10.0.1",
    "@angular/compiler-cli": "~10.0.3",
    "@angular/language-service": "~10.0.3",
    "@types/fs-extra": "^9.0.1",
    "@types/jasmine": "~3.5.11",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.0.20",
    "codelyzer": "^6.0.0",
    "concat": "^1.0.3",
    "fs-extra": "^9.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.0",
    "typescript": "3.9.6"
  }
}
