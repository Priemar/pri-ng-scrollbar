{
  "name": "pri-ng-scrollbar-demo",
  "version": "6.1.0",
  "license": "MIT",
  "description": "Custom overlay scrollbars using native scrolling mechanism, for angular outside angular change detection. now also usable as web-component in all web applications",
  "homepage": "https://github.com/Priemar/pri-ng-scrollbar",
  "author": {
    "name": "Mario Prieschl",
    "url": "http://github.com/Priemar"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Priemar/pri-ng-scrollbar.git"
  },
  "bugs": {
    "url": "https://github.com/Priemar/pri-ng-scrollbar/issues"
  },
  "keywords": [
    "scrollbar",
    "scroll",
    "custom",
    "native",
    "optimized",
    "webcomponent",
    "angular",
    "performance"
  ],
  "scripts": {
    "update": "ng update",
    "update:all": "ng update --all --force",
    "start": "ng run pri-ng-scrollbar-demo:serve --host 0.0.0.0 --port 4200",
    "start:lib": "ng run pri-ng-scrollbar:build:production --watch",
    "build": "npm run build:lib && ng run pri-ng-scrollbar-demo:build:production --base-href https://priemar.github.io/pri-ng-scrollbar/ && npm run build:lib",
    "build:lib": "ng run pri-ng-scrollbar:build:production && node build/copy.theme.js",
    "build:web-component": "npm run build:lib && ng run wc-pri-ng-scrollbar:build:production && node projects/web-component/bundle.js",
    "publish": "npm publish dist/pri-ng-scrollbar"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.2",
    "@angular/cdk": "^12.1.2",
    "@angular/common": "~12.1.2",
    "@angular/compiler": "~12.1.2",
    "@angular/core": "~12.1.2",
    "@angular/elements": "^12.1.2",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "~12.1.2",
    "@angular/material": "^12.1.2",
    "@angular/platform-browser": "~12.1.2",
    "@angular/platform-browser-dynamic": "~12.1.2",
    "@angular/router": "^12.1.2",
    "@webcomponents/custom-elements": "^1.4.1",
    "core-js": "^3.6.5",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.2",
    "@angular/cli": "~12.1.2",
    "@angular/compiler-cli": "~12.1.2",
    "@angular/language-service": "~12.1.2",
    "@types/fs-extra": "^9.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.0.20",
    "codelyzer": "^6.0.0",
    "concat": "^1.0.3",
    "fs-extra": "^9.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.1.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.0",
    "typescript": "4.3.5"
  }
}
